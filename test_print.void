import fmt

const test_print = fn(x: i32) -> i32 {
  fmt.println("Value: {:d}", x)
  return x * 2
}

const add_numbers = fn(a: i32, b: i32) -> i32 {
    return a + b
}

const main = fn() {
  fmt.println("Hello, World!")
  fmt.println("Number: {:d}", 42)
  fmt.println("Result: {:d}", test_print(21))
  fmt.println("This is a string! And this: {:d} is a number!", add_numbers(1, 5))
}